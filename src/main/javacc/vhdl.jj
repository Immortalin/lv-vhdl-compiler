void sequential_statement()       :
{}
{
  try {
    (
      LOOKAHEAD(3)
      wait_statement()
    | LOOKAHEAD(3)
      assertion_statement()
    | LOOKAHEAD(3)
      report_statement()
    | LOOKAHEAD(  [ label() ":" ] target() "<=" )
      signal_assignment_statement()
    | LOOKAHEAD(  [ label() ":" ] target() ":=" )
      variable_assignment_statement()
    | LOOKAHEAD(3)
      procedure_call_statement()
    | LOOKAHEAD(3)
      if_statement()
    | LOOKAHEAD(3)
      case_statement()
    | LOOKAHEAD(3)
      loop_statement()
    | LOOKAHEAD(3)
      next_statement()
    | LOOKAHEAD(3)
      exit_statement()
    | LOOKAHEAD(3)
      return_statement()
    | LOOKAHEAD(3)
      null_statement()

    /** 1076.1 extensions: */
    | break_statement()
    )
  }

  // Error handling: skip till next semicolon.
  catch (ParseException e)
  {
    error_skipto(SEMICOLON, "syntax error in sequential statement");
  } 
}


